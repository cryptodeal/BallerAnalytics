{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
    "build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**"],
			"outputs": ["dist/**", ".svelte-kit/**", ".vercel_build_output/**"]
		},
    "@balleranalytics/nba-api-ts#build": {
			"dependsOn": ["^build"],
			"inputs": [ "src/**"],
			"outputs": ["dist/**"]
		},
    "@balleranalytics/tf-neat#build": {
			"dependsOn": ["^build"],
			"inputs": [ "src/**"],
			"outputs": ["dist/**"]
		},
		"@balleranalytics/web#build": {
			"dependsOn": ["^build", "$VERCEL"],
			"inputs": ["src/**"],
			"outputs": [".vercel_build_output/**"]
		},
		"check": {
			"outputs": []
		},
		"format": {},
		"lint": {
			"dependsOn": ["format"],
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	},
  "globalDependencies": [".eslintrc.json", ".prettierrc", "tsconfig.json"]
}
