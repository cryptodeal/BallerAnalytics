{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"@balleranalytics/nba-api-ts#build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**"],
			"outputs": ["dist/**"]
		},
    "@balleranalytics/tf-neat#build": {
			"dependsOn": ["^build"],
			"inputs": [ "src/**"],
			"outputs": ["dist/**"]
		},
		"@balleranalytics/web#build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**"],
			"outputs": [".vercel_build_output/**"]
		},
    "build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**"],
			"outputs": ["files/**", "dist/**", ".svelte-kit/**", ".vercel_build_output/**"]
		},
		"check": {
			"outputs": []
		},
		"format": {},
		"lint": {
			"dependsOn": ["format"],
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": [".eslintrc.json", ".prettierrc", "tsconfig.json"]
}
