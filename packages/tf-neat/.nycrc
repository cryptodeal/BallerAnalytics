
{
  "extension": [
    ".ts"
  ],
  "include": [
    "*/**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "*.d.ts",
    "*.js",
    "src/scripts/**/*",
    "src/index.ts",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "all": true,
  "instrument": true
}