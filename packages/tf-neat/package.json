{
  "name": "@balleranalytics/tf-neat",
  "version": "0.0.1",
  "description": "implementation of neural network and neuroevolution tactics using tensorflow.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lintAction": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "test": "uvu -r tsm tests",
    "train:DQN": "node --experimental-specifier-resolution=node dist/scripts/train/DQN",
    "train:NEAT": "node --experimental-specifier-resolution=node dist/scripts/train/NEAT",
    "test:coverage": "c8 pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptodeal/BallerAnalytics/packages/tf-neat"
  },
  "keywords": [
    "NEAT",
    "Neuroevolution",
    "NeuralNet",
    "ML",
    "Machine",
    "Learning"
  ],
  "author": "cryptodeal",
  "license": "UNLICENSED",
  "devDependencies": {
    "@stdlib/types": "^0.0.14",
    "@tsconfig/node14": "^1.0.3",
    "@types/cytoscape": "^3.19.5",
    "@types/deep-equal": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "graphology-types": "^0.24.4",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tslib": "^2.4.0",
    "tsm": "^2.2.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "@balleranalytics/nba-api-ts": "workspace:0.1.0",
    "@stdlib/stdlib": "^0.0.96",
    "@tensorflow/tfjs": "^3.18.0",
    "@tensorflow/tfjs-backend-wasm": "^3.18.0",
    "@tensorflow/tfjs-core": "^3.18.0",
    "@tensorflow/tfjs-layers": "^3.18.0",
    "@tensorflow/tfjs-node": "^3.18.0",
    "cytoscape": "^3.22.1",
    "dayjs": "^1.11.3",
    "deep-equal": "^2.0.5",
    "dotenv": "^16.0.1",
    "graphology": "^0.24.1",
    "hm-cytoscape-all-paths": "^0.1.2",
    "hyperparameters": "^0.25.6",
    "nanoid": "3.3.4",
    "number-allocator": "^1.0.10",
    "rfdc": "^1.3.0",
    "seedrandom": "^3.0.5",
    "vectorious": "^6.1.4"
  }
}
