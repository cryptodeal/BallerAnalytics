{
  "name": "@balleranalytics/tf-neat",
  "version": "0.0.1",
  "description": "implementation of neural network and neuroevolution tactics using tensorflow.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lintAction": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "test": "uvu -r tsm tests",
    "train:DQN": "node --experimental-specifier-resolution=node dist/scripts/train/DQN",
    "train:NEAT": "node --experimental-specifier-resolution=node dist/scripts/train/NEAT",
    "test:coverage": "c8 pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptodeal/BallerAnalytics/packages/tf-neat"
  },
  "keywords": [
    "NEAT",
    "Neuroevolution",
    "NeuralNet",
    "ML",
    "Machine",
    "Learning"
  ],
  "author": "cryptodeal",
  "license": "UNLICENSED",
  "devDependencies": {
    "@stdlib/types": "^0.0.14",
    "@tsconfig/node14": "^1.0.1",
    "@types/deep-equal": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "tslib": "^2.4.0",
    "tsm": "^2.2.1",
    "tsup": "^5.12.9",
    "typescript": "^4.7.2",
    "uvu": "^0.5.3"
  },
  "dependencies": {
    "@balleranalytics/nba-api-ts": "workspace:*",
    "@stdlib/array": "^0.0.12",
    "@stdlib/array-base-zeros": "^0.0.2",
    "@tensorflow/tfjs": "^3.18.0",
    "@tensorflow/tfjs-backend-wasm": "^3.18.0",
    "@tensorflow/tfjs-core": "^3.18.0",
    "@tensorflow/tfjs-layers": "^3.18.0",
    "@tensorflow/tfjs-node": "^3.18.0",
    "abs": "link:@stdlib/math/base/special/abs",
    "array": "link:@stdlib/ndarray/array",
    "danfojs-node": "^1.1.1",
    "dayjs": "^1.11.2",
    "deep-equal": "^2.0.5",
    "dotenv": "^16.0.1",
    "exp": "link:@stdlib/math/base/special/exp",
    "float64": "link:@stdlib/array/float64",
    "is-nan": "link:@stdlib/assert/is-nan",
    "max": "link:@stdlib/math/base/special/fast/max",
    "min": "link:@stdlib/math/base/special/fast/min",
    "min-safe-integer": "link:@stdlib/constants/float64/min-safe-integer",
    "nanoid": "^3.3.4",
    "pow": "link:@stdlib/math/base/special/pow",
    "random": "^3.0.6",
    "rfdc": "^1.3.0",
    "seedrandom": "~2.4.4",
    "signbit": "link:@stdlib/number/float64/base/signbit",
    "sin": "link:@stdlib/math/base/special/sin",
    "tanh": "link:@stdlib/math/base/special/tanh",
    "zeros": "link:@stdlib/ndarray/zeros"
  }
}
