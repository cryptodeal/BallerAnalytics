{
  "name": "@balleranalytics/tf-neat",
  "version": "0.0.1",
  "description": "implementation of neural network and neuroevolution tactics using tensorflow.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lintAction": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "test": "uvu -r tsm tests",
    "test:coverage": "c8 pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptodeal/BallerAnalytics/packages/tf-neat"
  },
  "keywords": [
    "NEAT",
    "Neuroevolution",
    "NeuralNet",
    "ML",
    "Machine",
    "Learning"
  ],
  "author": "cryptodeal",
  "license": "UNLICENSED",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "c8": "^7.11.2",
    "chalk": "^5.0.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "tslib": "^2.4.0",
    "tsm": "^2.2.1",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "uvu": "^0.5.3"
  },
  "dependencies": {
    "@balleranalytics/nba-api-ts": "workspace:*",
    "@tensorflow/tfjs": "^3.16.0",
    "@tensorflow/tfjs-backend-wasm": "^3.16.0",
    "@tensorflow/tfjs-layers": "^3.16.0",
    "@tensorflow/tfjs-node": "^3.16.0",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "rfdc": "^1.3.0"
  }
}
