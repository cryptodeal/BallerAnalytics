{
	"name": "@balleranalytics/nba-api-ts",
	"version": "0.1.0",
	"description": "",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/cryptodeal/BallerAnalytics",
		"directory": "packages/nba-api-ts"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"dev": "tsc -p tsconfig.build.json",
		"lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
		"lintAction": "eslint '*/**/*.{js,ts,tsx}' --quiet",
		"mtgen": "mtgen --output ./src/db/interfaces ./src/db/models",
		"preview": "tsc -p tsconfig.build.json",
		"data:import:boxscores": "tsc -p tsconfig.build.json && node --experimental-specifier-resolution=node dist/scripts/import/boxscores",
		"data:cleanup:Game2:clearReferences": "tsc -p tsconfig.build.json && node --experimental-specifier-resolution=node dist/scripts/cleanup/Game2/clearReferences",
		"data:cleanup:Game2:fixTeamPeriodStats": "tsc -p tsconfig.build.json && node --experimental-specifier-resolution=node dist/scripts/cleanup/Game2/fixTeamPeriodStats",
		"start": "tsc -p tsconfig.build.json",
		"test": "uvu -r tsm tests",
		"test:coverage": "c8 pnpm test"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.45.0",
		"@types/cheerio": "^0.22.30",
		"@types/sharp": "^0.29.5",
		"cheerio": "^1.0.0-rc.10",
		"cross-fetch": "^3.1.4",
		"dayjs": "^1.10.7",
		"dotenv": "^10.0.0",
		"iso-3166-1-ts": "^0.2.2",
		"mongoose": "^6.1.4",
		"mongoose-slugger-plugin": "^3.0.3",
		"mongoose-tsgen": "^8.4.7",
		"nba": "^4.15.0",
		"sharp": "^0.29.3"
	},
	"devDependencies": {
		"@tsconfig/node14": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"c8": "^7.11.0",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"tslib": "^2.3.1",
		"tsm": "^2.2.1",
		"typescript": "^4.5.4",
		"uvu": "^0.5.2"
	}
}
