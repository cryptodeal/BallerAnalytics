{
	"name": "@balleranalytics/nba-api-ts",
	"version": "0.1.0",
	"description": "",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/cryptodeal/BallerAnalytics",
		"directory": "packages/nba-api-ts"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"types",
		"*.d.ts"
	],
	"scripts": {
		"build": "mtgen --output ./src/db/interfaces ./src/db/models && tsup",
		"build:fix": "esno scripts/postbuild.ts",
		"dev": "mtgen --output ./src/db/interfaces ./src/db/models && tsup",
		"lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
		"lintAction": "eslint '*/**/*.{js,ts,tsx}' --quiet",
		"mtgen": "mtgen --output ./src/db/interfaces ./src/db/models",
		"data:cleanup:sortSeasons": "node --experimental-specifier-resolution=node dist/scripts/cleanup/sortSeasons",
		"data:import:boxscores": "node --experimental-specifier-resolution=node dist/scripts/import/boxscores",
		"data:import:playerData": "node --experimental-specifier-resolution=node dist/scripts/import/playerData",
		"data:import:teamRosters": "node --experimental-specifier-resolution=node dist/scripts/import/teamRosters",
		"data:cleanup:Game2:clearReferences": "node --experimental-specifier-resolution=node dist/scripts/cleanup/Game2/clearReferences",
		"data:cleanup:Game2:fixTeamPeriodStats": "node --experimental-specifier-resolution=node dist/scripts/cleanup/Game2/fixTeamPeriodStats",
		"data:cleanup:Game2:gameOver": "node --experimental-specifier-resolution=node dist/scripts/cleanup/Game2/gameOver",
		"data:cleanup:Player2": "node --experimental-specifier-resolution=node dist/scripts/cleanup/Player2",
		"start": "tsup",
		"test": "uvu -r tsm tests",
		"test:coverage": "c8 pnpm test"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.72.0",
		"cheerio": "^1.0.0-rc.10",
		"cron": "^1.8.2",
		"cross-fetch": "^3.1.5",
		"dayjs": "^1.11.1",
		"dotenv": "^16.0.0",
		"iso-3166-1-ts": "^0.2.2",
		"mongoose": "6.2.7",
		"mongoose-slugger-plugin": "^3.0.3",
		"mongoose-tsgen": "^9.0.4",
		"nba": "^4.15.0",
		"sharp": "^0.30.3",
		"sportsdataverse": "^1.2.3"
	},
	"devDependencies": {
		"@tsconfig/node14": "^1.0.1",
		"@types/cheerio": "^0.22.31",
		"@types/cron": "^1.7.3",
		"@types/sharp": "^0.29.5",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"c8": "^7.11.0",
		"chalk": "^5.0.1",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"esno": "^0.14.1",
		"fast-glob": "^3.2.11",
		"husky": "^7.0.4",
		"tslib": "^2.3.1",
		"tsm": "^2.2.1",
		"tsup": "^5.12.5",
		"typescript": "^4.6.3",
		"uvu": "^0.5.3"
	}
}